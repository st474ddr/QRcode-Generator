{
  "name": "qr-code",
  "version": "1.0.0",
  "description": "An Electron application with Vue",
  "main": "./out/main/index.js",
  "author": "st474ddr",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "yarn run build && electron-builder --win --config",
    "build:mac": "yarn run build && electron-builder --mac --config",
    "build:linux": "yarn run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.1",
    "@electron-toolkit/utils": "^1.0.2",
    "qrcode.vue": "^3.3.3"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.33.0",
    "@rushstack/eslint-patch": "^1.1.1",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "electron": "^17.3.1",
    "electron-builder": "^22.14.13",
    "electron-notarize": "^1.2.1",
    "electron-reloader": "^1.2.3",
    "electron-vite": "^1.0.2",
    "eslint": "^8.12.0",
    "eslint-plugin-vue": "^8.5.0",
    "less": "^4.1.2",
    "prettier": "^2.6.1",
    "unplugin-auto-import": "^0.9.2",
    "unplugin-vue-components": "^0.21.1",
    "vite": "^2.9.1",
    "vue": "^3.2.31"
  }
}
